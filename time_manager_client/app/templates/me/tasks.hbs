{{#power-datepicker selected=dateStart onSelect=(action 'changeStartDate' value="date") as |dp|}}
  {{#dp.trigger tabindex="-1"}}
    <input type="text" class="my-input-class" readonly value={{moment-format dateStart}}>
  {{/dp.trigger}}

  {{#dp.content class="demo-datepicker-small"}}
    {{dp.nav}}
    {{dp.days}}
  {{/dp.content}}
{{/power-datepicker}}

